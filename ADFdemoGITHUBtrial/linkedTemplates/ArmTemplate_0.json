{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFdemoGITHUBtrial"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"DestinationcsvLinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DestinationcsvLinkedservice'"
		},
		"DestinationcsvLinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://blobstorageaccountdemo1.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/DestinationcsvLinkedservice')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('DestinationcsvLinkedservice_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('DestinationcsvLinkedservice_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/DelimitedText1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Order_history.csv",
						"folderPath": "Generalfiles",
						"container": "source"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Order ID",
						"type": "String"
					},
					{
						"name": "Market",
						"type": "String"
					},
					{
						"name": "Pair",
						"type": "String"
					},
					{
						"name": "Total Quantity",
						"type": "String"
					},
					{
						"name": "Remaining Quantity",
						"type": "String"
					},
					{
						"name": "Price Per Unit",
						"type": "String"
					},
					{
						"name": "Avg Price",
						"type": "String"
					},
					{
						"name": "Fee Amount",
						"type": "String"
					},
					{
						"name": "Side",
						"type": "String"
					},
					{
						"name": "Order Type",
						"type": "String"
					},
					{
						"name": "Status",
						"type": "String"
					},
					{
						"name": "Exchnage code",
						"type": "String"
					},
					{
						"name": "Created At",
						"type": "String"
					},
					{
						"name": "Updated At",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorage1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DelimitedTextSourceCSV')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "DestinationcsvLinkedservice",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "Order_destination.csv",
						"folderPath": "GenTargetFiles/destination",
						"fileSystem": "source"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/DestinationcsvLinkedservice')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Copypipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings"
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false,
							"enableSkipIncompatibleRow": false,
							"validateDataConsistency": true,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Order ID",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Order ID",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Market",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Market",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Pair",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Pair",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Total Quantity",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Total Quantity",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Remaining Quantity",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Remaining Quantity",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Price Per Unit",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Price Per Unit",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Avg Price",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Avg Price",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Fee Amount",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Fee Amount",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Side",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Side",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Order Type",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Order Type",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Status",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Status",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Exchnage code",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Exchnage code",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Created At",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Created At",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Updated At",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "Updated At",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "DelimitedText1",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DelimitedTextSourceCSV",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DelimitedText1')]",
				"[concat(variables('factoryId'), '/datasets/DelimitedTextSourceCSV')]"
			]
		}
	]
}